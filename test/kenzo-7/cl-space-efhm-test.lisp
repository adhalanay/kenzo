                                                 3 '(4 5 6))
                                   abar-degr abar))
            (setf gbar (cat-7:gbar 4 0 '(1 2 3) 0 '(4 5) 0 '(6) 0 '()))
            (unless (>= abar-degr 9)
              (setf cat-7:*tc* (cat-7:cmbn (+ 5 abar-degr) 1 gnrt))
              (setf cat-7:*bc* (cat-7:cmbn 4 1 gbar))
              (check-rdct))))
        (dotimes (i 10)
          (let ((abar (random-abar 6 4)))
            (setf abar-degr (apply #'+ (mapcar #'car (cat-7:abar-list abar))))
            (setf gnrt (cat-7:tnpr 4 (cat-7:tnpr 2 (cat-7:gbar 2 0 '(3) 0 '())
                                                 2 '(4 5))
                                   abar-degr abar))
            (setf gbar (cat-7:gbar 5 8 '(1 2 3) 0 '(4 5 6) 0 '(7 8) 0 '(9)
