(test cs-pre-left-hmeq-left-reduction
      (cat-7:cat-init)
      (let ((rdct (cat-7:cs-pre-left-hmeq-left-reduction (cat-7:k-z-1)))
            abar-degr
            gnrt
            gbar)
        (cat-7:pre-check-rdct rdct)
        (dotimes (i 10)
          (let ((abar (random-abar 8 4)))
            (setf abar-degr (apply #'+ (mapcar #'car (cat-7:abar-list abar))))
            (setf gnrt (cat-7:tnpr 5 (cat-7:tnpr 3 (cat-7:gbar 3 0 '(1 2) 0 '(3)
                                                               0 '())
                                                 2 '(4 5))
                                   abar-degr abar))
            (setf gbar (cat-7:gbar 2 0 '(1) 0 '()))
            (unless (>= abar-degr 9)
              (setf cat-7:*tc* (cat-7:cmbn (+ 5 abar-degr) 1 gnrt)
                    cat-7:*bc* (cat-7:cmbn 2 1 gbar))
              (check-rdct))))
        (dotimes (i 10)
          (let ((abar (random-abar 6 4)))
            (setf abar-degr (apply #'+ (mapcar #'car (cat-7:abar-list abar))))
            (setf gnrt (cat-7:tnpr 6 (cat-7:tnpr 3 (cat-7:gbar 3 0 '(1 2) 0 '(3)
                                                               0 '())
                                                 3 '(4 5 6))
                                   abar-degr abar))
            (setf gbar (cat-7:gbar 3 0 '(1 2) 1 '() 0 '()))
            (unless (>= abar-degr 8)
              (setf cat-7:*tc* (cat-7:cmbn (+ 6 abar-degr) 1 gnrt)
                    cat-7:*bc* (cat-7:cmbn 3 1 gbar))
